{
  "name": "Simple Chat Graph",
  "description": "A basic chat interaction graph with prompt and LLM",
  "nodes": [
    {
      "id": "node-1",
      "type": "prompt",
      "label": "User Input",
      "position": { "x": 100, "y": 100 },
      "config": {
        "template": "User: {input}\nAssistant: "
      }
    },
    {
      "id": "node-2",
      "type": "llm",
      "label": "GPT Response",
      "position": { "x": 400, "y": 100 },
      "config": {
        "model": "gpt-3.5-turbo",
        "temperature": 0.7,
        "max_tokens": 150
      }
    }
  ],
  "edges": [
    {
      "id": "edge-1",
      "source": "node-1",
      "target": "node-2",
      "label": "text"
    }
  ]
}
